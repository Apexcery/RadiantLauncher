<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Background Colors -->
    <SolidColorBrush x:Key="Background" Color="#FF191919" />
    <SolidColorBrush x:Key="BackgroundDark" Color="#FF0E0E0E" />
    <SolidColorBrush x:Key="BackgroundLight" Color="#FF212121" />
    <Color x:Key="BackgroundColor">#FF191919</Color>
    <Color x:Key="BackgroundDarkColor">#FF0E0E0E</Color>
    <Color x:Key="BackgroundLightColor">#FF212121</Color>

    <!-- Text Colors -->
    <SolidColorBrush x:Key="Text" Color="#EEEEEE" />
    <SolidColorBrush x:Key="TextDim" Color="#999999" />

    <!-- Match History Colors -->
    <Color x:Key="MatchDrawColor">#E8E8E8</Color>
    <SolidColorBrush x:Key="MatchDrawBrush" Color="#E8E8E8" />

</ResourceDictionary>